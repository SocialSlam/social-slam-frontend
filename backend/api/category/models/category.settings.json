{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "categories",
  "info": {
    "name": "Category"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "artists": {
      "via": "categories",
      "collection": "artist"
    },
    "events": {
      "via": "category",
      "collection": "slam"
    },
    "users": {
      "via": "preferred_categories",
      "plugin": "users-permissions",
      "collection": "user"
    }
  }
}
