{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "slams",
  "info": {
    "name": "Slam"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "slammers": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "performances",
      "dominant": true
    },
    "host": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "hosted_slams"
    },
    "description": {
      "type": "text"
    },
    "images": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "slam",
        "jam",
        "single_act",
        "collaborative_act"
      ]
    },
    "tickets": {
      "collection": "ticket",
      "via": "slam"
    },
    "admission": {
      "type": "float"
    }
  }
}
